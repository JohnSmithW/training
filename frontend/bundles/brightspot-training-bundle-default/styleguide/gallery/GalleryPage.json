{
  "_include": "/page/Page.json",
  "_template": "/gallery/GalleryPage.hbs",
  "datePublished": "{{date}}",
  "dateModified": "Modified: {{date}}",
  "headline": "{{words([5,15])}}",
  "subHeadline": "{{words([5,15])}}",
  "authorUrl": "#",
  "authorImage": {
    "_image": true
  },
  "authorName": "{{name}}",
  "authorBiography": "{{paragraphs(1)}}",
  "contributors": {
    "_include": "/link/Link.json"
  },
  "breadcrumbs": [
    {
      "_include": "/link/Link.json",
      "_repeat": 2
    }
  ],
  "tags": [
    {
      "_include": "/link/Link.json",
      "_repeat": 2
    }
  ],
  "slides": [
    {
      "_include": "/gallery/GallerySlide.json",
      "_repeat": 6
    }
  ],
  "galleryBody": "{{paragraphs([2,3])}}",
  "aside": [
    {
      "_include": "/page/list/PageListRightRailA.json"
    }
  ],
  "below": [
    {
      "_include": "/page/list/PageListStandardB.json",
      "listColumns234": "4",
      "items": [
        {
          "_include": "/page/promo/PagePromo.json",
          "_repeat": 4
        }
      ]
    }
  ]
}
